TC-O(n)
SC-(1)

class Solution:
    def sortColors(self, nums: List[int]) -> None:
        """
        Do not return anything, modify nums in-place instead.
        """
        i=0
        start=0
        end=len(nums)-1
        while i<end:  
            if nums[i]==0:
                nums[i],nums[start]=nums[start],nums[i]
                start+=1
                i+=1
            elif nums[i]==1:
                i+=1
            else:
                nums[i],nums[end]=nums[end],nums[i]
                end-=1
                
            
